{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "no-shadowed-variable": [
        true,
        {
          "class": true,
          "enum": true,
          "function": true,
          "interface": false,
          "namespace": true,
          "typeAlias": false,
          "typeParameter": false
        }
      ],
      "ordered-imports": false,
      "quotemark": [true, "double"],
      "trailing-comma": [true, { "multiline": "never", "singleline": "never" }]
    },
    "rulesDirectory": []
}
